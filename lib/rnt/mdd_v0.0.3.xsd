                                <xs:annotation>
                                    <xs:appinfo source="donnees_contextuelles/geolocalisation/adresse_bien"/>
                                    <xs:documentation>adresse du bien</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                        </xs:all>
                    </xs:complexType>
                </xs:element>
                <xs:element name="numero_projet" minOccurs="0" nillable="true">
                    <xs:annotation>
                        <xs:appinfo source="donnees_contextuelles/numero_projet"/>
                        <xs:documentation>numéro du projet de travaux</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:pattern value="[0-9]{8}"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="contexte">
                    <xs:annotation>
                        <xs:documentation>choix du contexte dans lequel les travaux sont proposés (audit, devis …)</xs:documentation>
                        <xs:appinfo source="donnees_contextuelles/contexte">
                            {
                            "audit": "audit",
                            "devis": "devis",
                            "facture": "facture"
                            }
                        </xs:appinfo>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="audit"/>
                            <xs:enumeration value="devis"/>
                            <xs:enumeration value="facture"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="usage_batiment">
                    <xs:annotation>
                        <xs:documentation>choix de l'usage, du type de bati (maison, appartement, immeuble ...)</xs:documentation>
                        <xs:appinfo source="donnees_contextuelles/usage_batiment">
                            {
                            "maison_individuelle": "maison individuelle",
                            "immeuble": "immeuble",
                            "appartement_chauffage_individuel": "appartement avec chauffage individuel",
                            "appartement_chauffage_collectif": "appartement avec chauffage collectif"
                            }
                        </xs:appinfo>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="maison_individuelle"/>
                            <xs:enumeration value="immeuble"/>
                            <xs:enumeration value="appartement_chauffage_individuel"/>
                            <xs:enumeration value="appartement_chauffage_collectif"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="aide_financiere_collection">
                    <xs:annotation>
                        <xs:documentation>Liste des aides financières visées</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="aide_financiere" maxOccurs="unbounded">
                                <xs:annotation>
                                    <xs:documentation>aide financière visée</xs:documentation>
                                    <xs:appinfo source="donnees_contextuelles/aide_financiere_collection/aide_financiere">
                                        {
                                        "cee": "cee",
                                        "mpr_geste": "mpr par geste",
                                        "mpr_ampleur": "mpr rénovation d'ampleur",
                                        "eco_ptz": "éco-ptz"
                                        }
                                    </xs:appinfo>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="cee"/>
                                        <xs:enumeration value="mpr_geste"/>
                                        <xs:enumeration value="mpr_ampleur"/>
                                        <xs:enumeration value="eco_ptz"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:all>
        </xs:complexType>
    </xs:element>
    <xs:complexType name="t_adresse">
        <xs:all>
            <xs:element name="adresse_brut">
                <xs:annotation>
                    <xs:documentation>libéllé l'adresse postale du bien saisi par le diagnostiqueur sans le code postal ni la commune (FORMAT ATTENDU : Numéro de l’adresse dans la voie + Indice de répétition associé au numéro (par exemple bis, a…) + Nom de la voie en minuscules accentuées)</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="255"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="code_postal_brut">
                <xs:annotation>
                    <xs:documentation>code postal de l'adresse brute saisie par le diagnostiqueur</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:pattern value="[0-9]{1}[A-Za-z0-9]{1}[0-9]{3}"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="nom_commune_brut">
                <xs:annotation>
                    <xs:documentation>nom de commune brute saisie par le diagnostiqueur</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="255"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="label_brut">
                <xs:annotation>
                    <xs:documentation>libellé complet de l'adresse postale du bien saisi par le diagnostiqueur (FORMAT ATTENDU : Numéro de l’adresse dans la voie + Indice de répétition associé au numéro (par exemple bis, a…) + Nom de la voie en minuscules accentuées + Code postal du bureau de distribution de la voie + Nom officiel de la commune actuelle</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="255"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="label_brut_avec_complement">
                <xs:annotation>
                    <xs:documentation>libellé complet d'adresse complète qui est l'adresse postale du bien précédé par l'ensemble des compléments d'adresses nécessaires à la bonne localisation du bien saisi par le diagnostiqueur (FORMAT ATTENDU : Compléments d'adresses + Numéro de l’adresse dans la voie + Indice de répétition associé au numéro (par exemple bis, a…) + Nom de la voie en minuscules accentuées + Code postal du bureau de distribution de la voie + Nom officiel de la commune actuelle)</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="255"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="statut_geocodage_ban">
                <xs:annotation>
                    <xs:documentation>statut de l'appareillement à la BAN de l'adresse</xs:documentation>
                    <xs:appinfo source="donnees_contextuelles/geolocalisation/adresses/t_adresse/statut_geocodage_ban">
                        {
                        "adresse_geocodee_ban": "adresse géocodée ban à l'adresse",
                        "adresse_non_geocodee_ban": "adresse non géocodée ban"
                        }
                    </xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="adresse_geocodee_ban"/>
                        <xs:enumeration value="adresse_non_geocodee_ban"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="ban_id" minOccurs="0" nillable="true">
                <xs:annotation>
                    <xs:documentation>clé d'intéropérabilité de la BAN referençant l'adresse. (id dans l'API adresse d'etalab). Attention Il ne s'agit pas de l'identifiant BAN stable (banId)</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="255"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="ban_id_ban_adresse" minOccurs="0" nillable="true">
                <xs:annotation>
                    <xs:documentation>identifant BAN stable officiel de l'adresse. Le nom du champs est celui du format BAL 1.4 ( https://aitf-sig-topo.github.io/voies-adresses/files/AITF_SIG_Topo_Format_Base_Adresse_Locale_v1.4.pdf ) (ce champs s'appelle banId dans l'API adresse d'etalab)</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="255"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="compl_nom_residence" minOccurs="0" nillable="true">
                <xs:annotation>
                    <xs:documentation>complement d'adresses : nom de la residence</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="255"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="compl_ref_batiment" minOccurs="0" nillable="true">
                <xs:annotation>
                    <xs:documentation>complement d'adresse : référence du bâtiment ( ex. A,B ,1,2,3,4)</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="255"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="compl_etage_appartement" type="xs:int" minOccurs="0" nillable="true">
                <xs:annotation>
                    <xs:documentation>complement d'adresses : etage du logement (cas de l'appartement) sous format entier 0 =RDC</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="compl_ref_cage_escalier" minOccurs="0" nillable="true">
                <xs:annotation>
                    <xs:documentation>complement d'adresses : reference de cage d'escalire&gt;</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="255"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="compl_ref_logement" minOccurs="0" nillable="true">
                <xs:annotation>
                    <xs:documentation>complement d'adresses : reference du logement dans le bâtiment (appartement ex. 10, A13, 3eme gauche)</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="255"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:all>
    </xs:complexType>
</xs:schema>
